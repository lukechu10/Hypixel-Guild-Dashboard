{
  "name": "hypixel-guild-dashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "concurrently --prefix-colors \"yellow,green\" -n server,client \"tsc --watch\" \"webpack --watch --mode=development\"",
    "build": "concurrently \"tsc\" \"webpack --mode=production\""
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "got": "^11.8.5",
    "http-errors": "^1.7.3",
    "moment": "^2.24.0",
    "mongodb": "^3.5.5",
    "morgan": "~1.9.1",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.3",
    "@types/http-errors": "^1.6.3",
    "@types/mongodb": "^3.5.4",
    "@types/morgan": "^1.9.0",
    "concurrently": "^5.2.0",
    "html-webpack-plugin": "^4.2.1",
    "lit-element": "^2.3.1",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
